{
    "multipart": [
        {   "when": { "default": "true" },
            "apply": { "model": "soviet:ventpipe_def" }
        },
     	{   "when": { "down": "true" },
            "apply": { "model": "soviet:ventpipe_down", "uvlock": true }
        },
     	{   "when": { "up": "true" },
            "apply": { "model": "soviet:ventpipe_up", "uvlock": true }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "soviet:ventpipe_north", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "soviet:ventpipe_east", "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "soviet:ventpipe_south","uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "soviet:ventpipe_west", "uvlock": true }
        }
    ]
}
